<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for post in site.posts %}
  <url>
    <loc>{{site.baseurl}}{{post.url}}</loc>
  </url>
  {% endfor %}

  {% for page in site.pages %}
  <url>
    <loc>{{site.baseurl}}{{page.url}}</loc>
  </url>
  {% endfor %}
</urlset>
